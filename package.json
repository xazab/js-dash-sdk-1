{
  "name": "xazab",
  "version": "3.17.0-dev.6",
  "description": "Xazab library for JavaScript/TypeScript ecosystem (Wallet, DAPI, Primitives, BLS, ...)",
  "main": "build/src/index.js",
  "unpkg": "dist/xazab.min.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "start:dev": "nodemon --exec 'npm run build && npm run test:unit'",
    "build": "npm run build:web",
    "build:web": "npm run build:ts && webpack --display-error-details",
    "build:ts": "tsc",
    "check-package": "npm run check-package:name && npm run check-package:version",
    "check-package:name": "test $(jq -r .name package.json) = $(jq -r .name package-lock.json)",
    "check-package:version": "test $(jq -r .version package.json) = $(jq -r .version package-lock.json)",
    "test": "npm run test:unit && npm run test:functional && npm run test:browsers",
    "test:browsers": "karma start ./karma.conf.js --single-run",
    "test:unit": "mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "test:functional": "npm run build && mocha --recursive tests/functional/**/*.js",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xazab/js-xazab-sdk.git"
  },
  "author": "Xazab Core Group <contact@xazab.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xazab/js-xazab-sdk/issues"
  },
  "homepage": "https://github.com/xazab/js-xazab-sdk#readme",
  "dependencies": {
    "@xazab/dapi-client": "~0.17.0-dev.5",
    "@xazab/xazabcore-lib": "~0.19.19",
    "@xazab/dpp": "~0.17.0-dev.9",
    "@xazab/wallet-lib": "~7.17.0-dev.5",
    "bs58": "^4.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/dirty-chai": "^2.0.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.4",
    "chai": "^4.2.0",
    "chance": "^1.1.6",
    "dirty-chai": "^2.0.1",
    "dotenv-safe": "^8.2.0",
    "karma": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "ts-loader": "^8.0.2",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.1"
  }
}
